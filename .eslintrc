{
	"env": {
		"es6": true,
		"browser" : true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
        "sourceType": "module"
    },
    "rules": {
    	"no-console": 0,
        "indent": [
            2,
            "tab"
        ],
        "quotes": false,
        "semi": [
            2,
            "always"
        ]
    }
}
